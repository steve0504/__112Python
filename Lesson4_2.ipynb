   "source": [
    "#自訂的function,1個參數\n",
    "def get_factor (para1):\n",
    "    #求因數\n",
    " print(f\"{para1}的因數是\")\n",
    " for i in range(1, para1+1):\n",
    "    if para1 % i ==0:\n",
    "        print(i)\n",
    "\n",
    " print(\"_______結束\")  \n"
   ]
